<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_isp_provisioning_jobs" model="ir.cron">
        <field name="name">ISP Provisioning Jobs</field>
        <field name="model_id" ref="model_isp_provisioning_job"/>
        <field name="state">code</field>
        <field name="code">model._cron_run_pending_jobs()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="active">True</field>
    </record>
    <record id="ir_cron_isp_plan_change_apply" model="ir.cron">
        <field name="name">ISP Apply Plan Changes</field>
        <field name="model_id" ref="model_isp_plan_change_request"/>
        <field name="state">code</field>
        <field name="code">model._cron_apply_plan_changes()</field>
        <field name="interval_number">6</field>
        <field name="interval_type">hours</field>
        <field name="active">True</field>
    </record>
</odoo>
